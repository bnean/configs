! *background:            #272827
*background:            #d0d0d0
*foreground:            #657b83
! *foreground:            #f81ce5 
*fading:                15
*fadeColor:             black
*cursorColor:           #f81ce5  
*pointerColorBackground:#2B2C2B
*pointerColorForeground:#16A085

!! black dark/light
*color0:                #000000
*color8:                #808080

!! red dark/light
*color1:                #dc322f
*color9:                #ff0000

!! green dark/light
*color2:                #859900
*color10:               #33ff00

!! yellow dark/light
*color3:                #b58900
*color11:               #ffff00

!! blue dark/light
*color4:                #268bd2
*color12:               #0066ff

!! cyan dark/light
*color6:                #2aa198
*color14:               #00ffff

!! white dark/light
*color7:                #d0d0d0
*color15:               #ffffff

Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
Xcursor.theme: Chameleon-Pearl-Regular-0.5
Xcursor.size:  0

XTerm*background: #272827
XTerm*foreground: #378735
XTerm*reverseVideo: on
XTerm*faceName:Terminatus:size=8:antialias=true
XTerm*selectToClipboard: true

! URxvt.font: xft:DejaVu Sans Mono:size=12
URxvt.letterSpace: -1
URxvt*font: xft:DejaVu Sans Mono:antialias=true:pixelsize=12
URxvt*boldFont: xft:DejaVu Sans Mono:style=bold:antialias=true:pixelsize=12
URxvt*italicFont: xft:DejaVu Sans Mono:style=italic:antialias=true:pixelsize=12
URxvt*boldItalicFont: xft:DejaVu Sans Mono:style=bolditalic:antialias=true:pixelsize=12
URxvt.depth: 32
URxvt.background: [80]#000000
URxvt*scrollBar:                      false
URxvt*mouseWheelScrollPage:           false
URxvt*cursorBlink:                    false
URxvt*background:                     black
URxvt*foreground:                     grey
URxvt*saveLines:                      5000
URxvt*cursorUnderline:                true

! Activate tabs
! URxvt.perl-ext-common: ...,default,tabbedex,...
! URxvt.tabbed.tabbar-fg: 6
! URxvt.tabbed.tabbar-bg: 0
! URxvt.tabbed.tab-fg:    1
! URxvt.tabbed.tab-bg:    0
! URxvt.tabbed.title: no

! for 'fake' transparency (without Compton) uncomment the following three lines
! URxvt*inheritPixmap:                  true
! URxvt*transparent:                    true
! URxvt*shading:                        138

